	"scripts": {
		"commit": "cz",
		"compile": "tsc -p src",
		"lint": "xo",
		"prepare": "husky install;",
		"prepublishOnly": "npm run compile",
		"release": "release-it",
		"test": "tsc -p test && mocha",
		"test:dev": "mocha",
		"test:watch": "tsc-watch -p test --onSuccess 'mocha'",
		"watch": "tsc-watch -p src",
		"watch:test": "tsc-watch -p test"
	},
